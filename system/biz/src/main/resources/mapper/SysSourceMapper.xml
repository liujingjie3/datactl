<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.SysSourceMapper">

    <resultMap id="BaseResultMap" type="generator.domain.SysSource">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="sourceName" column="source_name" jdbcType="VARCHAR"/>
            <result property="souceCode" column="souce_code" jdbcType="VARCHAR"/>
            <result property="description" column="description" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="TINYINT"/>
            <result property="delFlag" column="del_flag" jdbcType="TINYINT"/>
            <result property="opreateDate" column="opreate_date" jdbcType="DATE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,source_name,souce_code,
        description,status,del_flag,
        opreate_date
    </sql>
</mapper>
